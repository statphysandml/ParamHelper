@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Handle nlohmann_json dependency
if(@PARAMHELPER_USE_SYSTEM_JSON@)
    find_dependency(nlohmann_json REQUIRED)
endif()

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# Add alias for backwards compatibility if needed
if(NOT TARGET @PROJECT_NAME@::paramhelper AND TARGET @PROJECT_NAME@::@PROJECT_NAME@)
    add_library(@PROJECT_NAME@::paramhelper ALIAS @PROJECT_NAME@::@PROJECT_NAME@)
endif()

message(STATUS "Found @PROJECT_NAME@ @PROJECT_VERSION@")